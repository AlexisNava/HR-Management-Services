type User {
  id: ID! @id
  email: String! @unique
  password: String!
  name: String!
  lastName: String!
  mothersName: String
  phoneNumber: String
  isAdmin: Boolean! @default(value: false)
  isActive: Boolean! @default(value: true)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}