type User {
  id: ID! @id
  password: String!
  isAdmin: Boolean! @default(value: false)
  isActive: Boolean! @default(value: true)
  personalInformation: PersonalInformation @relation(link: INLINE)
}

type PersonalInformation {
  id: ID! @id
  email: String! @unique
  names: String!
  lastName: String!
  mothersName: String!
  phoneNumber: Int!
}
